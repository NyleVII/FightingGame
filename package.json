{
	"name": "fighting-game",
	"version": "0.0.1",
	"scripts": {
		"start": "node server/main.js",
		"start:db": "mongod --config db/config",
		"install:dev": "npm install",
		"install:prod": "npm install --production",
		"build": "tar -zcvf builds/$(date \"+%Y-%m-%d-%H%M%S\").tar.gz server client db/config package.json"
	},
	"dependencies": {
		"mongodb": "latest",
		"websocket": "latest"
	},
	"devDependencies": {
		"eslint": "latest"
	}
}
